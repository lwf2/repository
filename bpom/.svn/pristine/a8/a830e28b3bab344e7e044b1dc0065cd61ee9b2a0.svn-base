package com.aspire.bpom.constants;

public interface ErrorReturnCode extends ReturnCode{


	/**
	 * 验证签名错误
	 */
	int CHECK_SIGN_ERROR = 4010001;
	/**
	 * 支付请求超过有效期
	 */
	int PAY_REQUEST_TIMEOUT = 4010002;
	/**
	 * 未找到支付记录
	 */
	int ORDER_NOT_FOUND = 4010003;
	/**
	 * 未完成支付不能退款
	 */
	int REFUND_NOT_ALLOWED = 4010004;
    /**
     * 超过退款允许时间
     */
    int REFUND_TIMEOUT = 4010005;

    /**
     * 请求参数校验错误
     */
    int ERROR_REQUEST_PARAM = 4010006;
    
    /**
     * 订单平台内部错误-数据操作异常
     */
    int ERROR_INTERNAL_DB = 4010007;
    
    /**
     * 订单平台内部错误-支付网关响应异常
     */
    int ERROR_REQUEST_PAYMENT_GATEWAY = 4010008;
    
    /**
     * 同支付网关通信超时
     */
    int ERROR_CONNECT_OUT_PAYGATE = 4010009;
    
    /**
     * 同任我看业务平台通信超时
     */
    int ERROR_CONNECT_OUT_RWK = 4010010;
    
    /**
     * 处理异常
     */
    int EXCEPTION = 4010011;
    
    /**
     * 订单不存在
     */
    int ORDER_NOT_EXIST = 4010012;
    
    /**
     * 订单已关闭
     */
    int ORDER_CLOSED = 4010013;
}
